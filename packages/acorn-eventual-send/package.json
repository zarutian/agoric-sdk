{
  "name": "@agoric/acorn-eventual-send",
  "version": "2.1.6",
  "description": "Eventual send (wavy dot) parser plugin for Acorn",
  "parsers": {
    "js": "mjs"
  },
  "main": "index.js",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:xs": "exit 0",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint-check": "eslint '**/*.js'"
  },
  "devDependencies": {
    "acorn": "^8.0.5",
    "ava": "^3.12.1",
    "esm": "^3.2.25",
    "nyc": "^15.1.0",
    "rollup": "^1.16.7"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Agoric/agoric-sdk"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/acorn-eventual-send#readme",
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "esm"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
